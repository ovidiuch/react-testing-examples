{
  "scripts": {
    "lint": "eslint '**/*.js'",
    "test:jest-enzyme": "jest --config tests/jest-enzyme/jest.config.js",
    "test:jest-enzyme:watch": "yarn test:jest-enzyme --watch",
    "test:jest-rtl": "jest --config tests/jest-rtl/jest.config.js",
